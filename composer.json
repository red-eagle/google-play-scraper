{
    "name": "red-eagle/google-play-scraper",
    "version": "1.0.1",
    "type": "library",
    "description": "A PHP scraper to get app data from Google Play. Fork of raulr/google-play-scraper.",
    "keywords": ["google play", "scraper"],
    "homepage": "https://github.com/red-eagle/google-play-scraper",
    "license": "MIT",
    "require": {
        "fabpot/goutte": "^3.0",
        "campo/random-user-agent": "^1.2"
    },
    "authors": [
        {
            "name": "Raul Rodriguez",
            "email": "raul@raulr.net"
        },
        {
            "name": "Red Eagle",
            "email": "rosso.aquila@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raulr\\GooglePlayScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raulr\\GooglePlayScraper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "friendsofphp/php-cs-fixer": "^1.10",
        "mockery/mockery": "^0.9.4"
    }
}
